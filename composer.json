{
  "name": "chbakouras/laravel-crud-api-generator",
  "description": "Laravel Crud Api Generator",
  "keywords": [
    "laravel",
    "crud",
    "api",
    "generator"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Chrisostomos Bakouras",
      "email": "chbakouras@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0.0",
    "illuminate/support": "5.6.*",
    "illuminate/database": "5.6.*",
    "illuminate/console": "5.6.*",
    "illuminate/http": "5.6.*"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.0",
    "mockery/mockery": "~0.9"
  },
  "autoload": {
    "psr-4": {
      "Chbakouras\\CrudApiGenerator\\": "src/"
    },
    "files": [
      "src/Helper/functions.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Chbakouras\\CrudApiGenerator\\LaravelCrudApiGeneratorServiceProvider"
      ]
    }
  },
  "minimum-stability": "stable"
}

